Preferences #unprotectedWifi:indicator:unchecked { image: url(":/Common/radio-OFF.png") }

Preferences #connectionPage QComboBox            { font-size: 14px; color: #7c85a2; margin-right: 15px; background: transparent; border-bottom: 1px solid black; }
