Preferences #generalPage QRadioButton:indicator:unchecked { image: url(":/Common/radio-OFF.png") }

Preferences #generalPage QCheckBox        { font-size: 14px; color: #7c85a2; }
Preferences #generalPage QCheckBox:indicator { min-width: 16px; max-width: 16px; min-height: 16px; max-height: 16px; }
Preferences #generalPage QCheckBox:indicator:checked { image: url(":/Common/radio-ON.png") }
Preferences #generalPage QCheckBox:indicator:unchecked { image: url(":/Common/radio-OFF.png") }

Preferences #freeTrial[styleHint="trial"]    { font-size: 14px; color: #7c85a2; }
Preferences #freeTrial[styleHint="activated"]{ font-size: 14px; color: white; }
